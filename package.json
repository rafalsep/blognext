{
  "name": "gooddev",
  "version": "0.1.0",
  "description": "Blog about programming best practices",
  "main": "pages/blog.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:rafalsep/blognext.git"
  },
  "engines": {
    "npm": ">=5",
    "node": ">=8"
